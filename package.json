{
   "name": "fetch-json",
   "description": "A wrapper around Fetch just for JSON",
   "version": "2.1.3",
   "license": "MIT",
   "main": "fetch-json.js",
   "browser": "fetch-json.min.js",
   "repository": "github:center-key/fetch-json",
   "homepage": "https://github.com/center-key/fetch-json",
   "bugs": "https://github.com/center-key/fetch-json/issues",
   "docs": "https://github.com/center-key/fetch-json#readme",
   "contributors": [
      "KahWee Teng (https://twitter.com/kahwee)",
      "Dem Pilafian (https://twitter.com/dempilafian)",
      "Calvin Settachatgul (https://github.com/calvinsettachatgul)"
   ],
   "keywords": [
      "browser",
      "fetch",
      "node-fetch",
      "get",
      "http",
      "javascript",
      "json",
      "post",
      "rest"
   ],
   "jshintConfig": {
      "esversion": 6,
      "strict": "implied",
      "undef": true,
      "unused": true,
      "browser": true,
      "mocha": true,
      "node": true,
      "globals": {
         "fetch": true,
         "window": true
      }
   },
   "scripts": {
      "lint": "jshint gulpfile.js spec/*.js docs/*.js fetch-json.js",
      "pretest": "npm run lint && gulp build",
      "test": "mocha --timeout 10000 spec/*.js",
      "quick": "npm run lint && mocha --bail spec/node.js"
   },
   "dependencies": {},
   "devDependencies": {
      "@babel/core": "~7.2",
      "@babel/preset-env": "~7.2",
      "@fortawesome/fontawesome-free": "~5.5",
      "dna.js": "~1.5",
      "gulp": "~4.0",
      "gulp-babel": "~8.0",
      "gulp-babel-minify": "~0.5",
      "gulp-header": "~2.0",
      "gulp-rename": "~1.4",
      "gulp-replace": "~1.0",
      "gulp-size": "~3.0",
      "jquery": "~3.3",
      "jsdom": "~13.0",
      "jshint": "~2.9",
      "merge-stream": "~1.0",
      "mocha": "~5.2",
      "node-fetch": "~2.3",
      "promise-polyfill": "~8.1",
      "web-ignition": "~1.0",
      "whatwg-fetch": "~3.0"
   }
}
