{
   "name": "browser-fetch-json",
   "description": "A wrapper around Fetch just for JSON",
   "version": "0.2.2",
   "license": "MIT",
   "main": "fetch-json.js",
   "repository": "github:center-key/browser-fetch-json",
   "homepage": "https://github.com/center-key/browser-fetch-json",
   "bugs": "https://github.com/center-key/browser-fetch-json/issues",
   "docs": "https://github.com/center-key/browser-fetch-json/blob/master/README.md",
   "keywords": [
      "browser",
      "fetch",
      "get",
      "http",
      "javascript",
      "json",
      "post",
      "rest"
   ],
   "author": {
      "name": "Center Key",
      "url": "https://centerkey.com"
   },
   "eslintConfig": {
      "extends": "eslint:recommended",
      "env": {
         "es6": true,
         "browser": true,
         "node": true,
         "mocha": true
      },
      "rules": {
         "no-console": "off",
         "quotes": [
            "warn",
            "single"
         ],
         "semi": [
            "warn",
            "always"
         ]
      }
   },
   "scripts": {
      "lint": "eslint gulpfile.js spec-*.js fetch-json.js",
      "pretest": "npm run lint && gulp build",
      "test": "mocha --timeout 10000 spec-*.js",
      "quick": "npm run lint && mocha --bail spec-node.js"
   },
   "dependencies": {},
   "devDependencies": {
      "ajv": "~6.5",
      "babel-core": "~6.26",
      "eslint": "~5.5",
      "gulp": "~4.0",
      "gulp-babel": "~7.0",
      "gulp-babel-minify": "~0.4",
      "gulp-header": "~2.0",
      "gulp-rename": "~1.4",
      "gulp-replace": "~1.0",
      "gulp-size": "~3.0",
      "jsdom": "~12.0",
      "merge-stream": "~1.0",
      "mocha": "~5.2",
      "node-fetch": "~2.2",
      "whatwg-fetch": "~3.0"
   }
}
